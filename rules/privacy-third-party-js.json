{
    "title": "Block third-party scripts",
    "tag": "block-scripts",
    "pattern": {
        "scheme": [
            "http",
            "https"
        ],
        "host": [
            "*.google-analytics.com",
            "*.doubleclick.net",
            "*.facebook.net"
        ],
        "origin": "cross-domain"
    },
    "types": [
        "script"
    ],
    "action": "block",
    "active": true
}